<?xml version="1.0"?>
<!DOCTYPE statedata SYSTEM "stateData.dtd">
<statedata>
	<controlData identifier="openloop" 
	             mode="PERCENT_OUTPUT"/>
	<controlData identifier="closedloopblue"
				 mode="VELOCITY_RPS"
				 proportional="0.03"
				 feedforward="0.28"/>
	<controlData identifier="closedloopyellow"
				 mode="VELOCITY_RPS"
				 proportional="0.03"
				 feedforward="0.18"/>
	<controlData identifier="closedloopgreen"
				 mode="VELOCITY_RPS"
				 proportional="0.03"
				 feedforward="0.09"/>
	<controlData identifier="closedloopred"
				 mode="VELOCITY_RPS"
				 proportional="0.03"
				 feedforward="0.37"/>
	<mechanismTarget stateIdentifier="SHOOTEROFF"
	                 controlDataIdentifier="openloop"
					 value="0.0"/>
	<mechanismTarget stateIdentifier="SHOOTERGETREADY"
	                 controlDataIdentifier="openloop"
					 value="0.5"
					 secondValue="0.5"/>

	<mechanismTarget stateIdentifier="SHOOTERSHOOTGREEN"
					 controlDataIdentifier="closedloopgreen"
					 value="10"
					 secondValue="10"/>

	<mechanismTarget stateIdentifier="SHOOTERSHOOTYELLOW"
	                 controlDataIdentifier="closedloopyellow"
					 value="20"
					 secondValue="20"/>

	<mechanismTarget stateIdentifier="SHOOTERSHOOTBLUE"
					 controlDataIdentifier="closedloopbluer"
					 value="30"
					 secondValue="30"/>

	<mechanismTarget stateIdentifier="SHOOTSHOOTRED"
					 controlDataIdentifier="closedloopred"
					 value="40"
					 secondValue="40"/>

</statedata>
